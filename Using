class Student:
    def __init__(self, name, marks):
        self.name = name
        self.marks = marks

    def get_grade(self):
        return 'A' if self.marks >= 90 else 'B' if self.marks >= 80 else 'C' if self.marks >= 70 else 'D' if self.marks >= 60 else 'F'

s = Student(input("Name: "), float(input("Marks: ")))
print(f"{s.name} received grade {s.get_grade()}.")





